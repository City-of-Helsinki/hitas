{
    "overrides": [
        {
            "files": ".prettierrc.json",
            "options": { "parser": "json" }
        }
    ],
    "trailingComma": "es5",
    "printWidth": 120,
    "singleQuote": false,
    "bracketSpacing": false,
    "parser": "typescript",
    "singleAttributePerLine": true,
    "importOrder": [
        "^react$",
        "<THIRD_PARTY_MODULES>",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}

